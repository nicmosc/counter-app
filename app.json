{
  "expo": {
    "name": "counter-app",
    "description": "Simple counter app.",
    "slug": "counter-app",
    "privacy": "public",
    "sdkVersion": "27.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#191B29"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nicmosc.counter"
    },
    "android": {
      "package": "com.nicmosc.counter"
    },
    "primaryColor": "#191B29"
  }
}
